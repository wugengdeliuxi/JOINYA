{
  "version": 2,
  "buildCommand": "echo 'No build required for API'",
  "outputDirectory": ".",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
