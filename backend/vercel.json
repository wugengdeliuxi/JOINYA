{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@4.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
